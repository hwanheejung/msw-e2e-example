{
  "name": "msw-e2e-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "msw-auto-mock:generate": "tsx tests/mocks/mock-generator.ts",
    "e2e-autogen:generate": "e2e-autogen generate --sheets https://docs.google.com/spreadsheets/d/1EjjINCvD-4FkSETb_Z88qauXUpF7NU9y7jXN0RTNTm4/edit --output ./tests/e2e/__generated-stub__",
    "e2e-autogen:update": "e2e-autogen update --sheets https://docs.google.com/spreadsheets/d/1EjjINCvD-4FkSETb_Z88qauXUpF7NU9y7jXN0RTNTm4/edit --results ./tests/reporters/results.json"
  },
  "dependencies": {
    "@playwright/test": "^1.53.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@dataai/msw-auto-mock": "^0.31.0",
    "@eslint/js": "^9.30.1",
    "@faker-js/faker": "^9.9.0",
    "@msw/playwright": "^0.4.2",
    "@types/node": "^24.0.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "msw": "^2.10.3",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.3"
  },
  "packageManager": "yarn@4.8.1",
  "resolutions": {
    "@dataai/msw-auto-mock": "portal:../../voice-emr/msw-auto-mock"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
